{
  "name": "zerotwo",
  "version": "0.3.4",
  "description": "Refined state containter for Vue.js",
  "repository": {
    "url": "egoist/zerotwo",
    "type": "git"
  },
  "main": "dist/zerotwo.cjs.js",
  "module": "dist/zerotwo.es.js",
  "cdn": "dist/zerotwo.js",
  "unpkg": "dist/zerotwo.js",
  "jsdelivr": "dist/zerotwo.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint src/*.js",
    "test": "npm run lint && tyu",
    "test:cov": "npm run lint && tyu -- --coverage",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "egoist",
    "email": "0x142857@gmail.com"
  },
  "license": "MIT",
  "bili": {
    "format": [
      "cjs",
      "umd",
      "es",
      "umd-min"
    ],
    "moduleName": "zerotwo",
    "global": {
      "vue": "Vue"
    },
    "js": "buble",
    "exports": "named",
    "buble": {
      "transforms": {
        "dangerousForOf": true
      }
    }
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "babel-eslint": "^8.2.2",
    "bili": "^3.0.2",
    "eslint": "^4.18.2",
    "eslint-config-rem": "^4.0.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-vue": "^4.2.2",
    "poi": "^9.6.13",
    "tyu": "^1.0.0",
    "vue": "^2.5.13"
  },
  "dependencies": {
    "nano-assign": "^1.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "extends": [
      "xo",
      "rem",
      "plugin:vue/essential"
    ],
    "env": {
      "jest": true
    }
  },
  "babel": {
    "presets": [
      "poi"
    ]
  }
}
